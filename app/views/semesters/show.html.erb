<h5><%= link_to("Back to List", semesters_path, :class => 'back-link') %></h5>
<% @page_title = "Show Details" %>

<h3>Semester Details</h3>

<p><i> Showing details of the semester</p></i>


<table class="table">
	<tr>
		<th>Name</th>
		<td><%= @semester.name %></td>
	</tr>
	<tr>
		<th>Start</th>
		<td><%= @semester.dates.begin %></td>
	</tr>
	<tr>
		<th>End</th>
		<td><%= @semester.dates.end %></td>
	</tr>
	<tr>
		<th>Created at</th>
		<td><%= @semester.created_at %></td>
	</tr>
	<tr>
		<th>Last Updated on</th>
		<td><%= @semester.updated_at %></td>
	</tr>
</table>