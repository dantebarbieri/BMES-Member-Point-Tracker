<h5><%= link_to("Back to List", admins_path, :class => 'back-link') %></h5>

<h3>Admin Details</h3>

<p><i> This shows the details of each admins </p></i>
<table class="table" summary="user details">

    <tr class="header">
        <th>Attribute</th>
        <th>Value</th>
    </tr>
    <tr>
        <th>Name</th>
        <td><%=@user.name %></td>
    </tr>
    <tr>
        <th>Email</th>
        <td><%=@user.email %></td>
    </tr>
    <tr>
        <th>Class Year</th>
        <td><%=@user.class_year %></td>
    </tr>
    <tr>
        <th>Role</th>
        <td><%=@user.role %></td>
    </tr>
    <tr>
        <th>Dues Paid?</th>
        <td><%=@user.paid_dues? %></td>
    </tr>
    <tr>
        <th>Total Points</th>
        <td><%=@user.total_points %></td>
    </tr>
    <tr>
        <th>Accomplishment Points</th>
        <td><%=@user.accomplishment_points %></td>
    </tr>
    <tr>
        <th>Attendance Points</th>
        <td><%=@user.attendance_points %></td>
    </tr>
    <tr>
        <th>Manually Assigned Points</th>
        <td><%=@user.manual_points_received %></td>
    </tr>

</table>


