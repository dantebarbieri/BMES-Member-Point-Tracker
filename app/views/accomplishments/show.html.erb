<h5><%= link_to("Back to List", accomplishments_path, :class => 'back-link') %></h5>

<% @page_title = "Details" %>

<h3>Details</h3>

<i><p>Viewing Details of the Following Accomplishment: <%= @accomplishments.name %></p></i>

<table class="table data-inline">
	<tr>
		<th>Name</th>
		<td><%= @accomplishments.name %></td>
	</tr>
	<tr>
		<th>Description</th>
		<td><%= @accomplishments.description %></td>
	</tr>
	<tr>
		<th>Points</th>
		<td><%= @accomplishments.points %></td>
	</tr>
	<tr>
		<th>Is Dues?</th>
		<td><%= @accomplishments.is_dues %></td>
	</tr>
	<tr>
		<th>Created at</th>
		<td><%= @accomplishments.created_at %></td>
	</tr>
	<tr>
		<th>Last Updated on</th>
		<td><%= @accomplishments.updated_at %></td>
	</tr>
</table>